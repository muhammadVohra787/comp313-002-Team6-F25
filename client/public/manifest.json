{
  "manifest_version": 3,
  "name": "Jobmate AI",
  "version": "1.0",
  "description": "AI-powered job cover letter generator",
  "permissions": ["tabs", "scripting", "storage"],
  "key": "",
  "action": {
    "default_popup": "index.html"
  },
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}
